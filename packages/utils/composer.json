{
    "name": "masmerise/laravel-matchmaking-utils",
    "description": "Utilities to accelerate development with (Laravel) Matchmaking.",
    "license": "MIT",
    "homepage": "https://github.com/masmerise/laravel-matchmaking",
    "support": {
        "issues": "https://github.com/masmerise/laravel-matchmaking/issues",
        "source": "https://github.com/masmerise/laravel-matchmaking"
    },
    "require": {
        "php": "~8.4"
    },
    "autoload": {
        "psr-4": {
            "Masmerise\\Matchmaking\\Utility\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masmerise\\Matchmaking\\Utility\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}